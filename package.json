  "devDependencies": {
    "@types/puppeteer": "^3.0.1",
    "nodemon": "^2.0.4",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.2",
    "typescript": "^3.9.7",
    "yarn-or-npm": "^3.0.1"
  },
  "dependencies": {
    "puppeteer": "^5.2.1"
  }
}
